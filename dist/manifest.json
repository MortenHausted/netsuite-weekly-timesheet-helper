{
  "manifest_version": 3,
  "name": "Netsuite - Weekly Timesheet Helper",
  "version": "0.0.1",
  "description": "Helps apply hours to Weekly Timesheets",
  "permissions": ["activeTab", "scripting"],
  "icons": {
    "128": "assets/icon128.png"
  },
  "content_scripts": [
    {
      "matches": ["https://*.app.netsuite.com/*"],
      "js":["index.js"],
      "all_frames": true
    }
  ]
}
